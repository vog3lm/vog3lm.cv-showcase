{
  "hosting": {
    "public": "pages",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {"source":"/qr" ,"function":"qr"}
     ,{"source":"/mvp","function":"mvp"}
     ,{"source":"/mvc","function":"mvc"}
    ],
    "cleanUrls":true
  },
  "functions": {
    "source": "functions",
    "predeploy": [
      "npm --prefix $RESOURCE_DIR run lint"
    ]
  }
}
