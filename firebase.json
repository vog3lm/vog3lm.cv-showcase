{
  "hosting": {
    "public": "pages",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/hello",
        "function": "helloWorld"
      }
    ],
    "cleanUrls": true
  },
  "functions": {
    "source": "functions",
    "predeploy": [
      "npm --prefix $RESOURCE_DIR run lint"
    ]
  }
}
