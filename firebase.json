{
  "hosting": {
    "public": "pages",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {"source":"/qr","function":"qr"}
    ],
    "cleanUrls": true
  },
  "functions": {
    "source": "functions",
    "predeploy": [
      "npm --prefix $RESOURCE_DIR run lint"
    ]
  }
}
